---
import MainLayout from '../layouts/MainLayout.astro';
---

<MainLayout>
  <!-- Hero -->
<section
  class="relative bg-cover bg-center bg-no-repeat min-h-[520px] sm:min-h-[620px] flex items-center"
  style={`background-image: url('${import.meta.env.BASE_URL}img/city-hero.avif')`}
>
    <div class="absolute inset-0 bg-ink/75"></div>

    <div class="container-page pt-12 pb-12 sm:pt-16 sm:pb-16 relative z-10">
      <div class="mx-auto max-w-3xl text-center">
        <div class="inline-flex items-center justify-center rounded-full border border-white/20 px-3 py-1">
          <p class="text-xs font-semibold text-white">
            Same-day settlement available for eligible transactions
          </p>
        </div>

        <h1 class="mt-4 text-balance text-3xl font-bold leading-tight text-white sm:text-4xl lg:text-5xl">
          <span>Built for Brokers. </span><br />
          <span>Designed for Business.</span>
        </h1>

        <p class="mt-3 text-pretty text-base text-white/75 sm:text-lg">
          <span>Fast, flexible business finance with same-day settlement capabilities. </span><br />
          <span>We assess deals on merit, not just algorithms.</span>
        </p>

        <div class="mt-6 flex flex-col items-center justify-center gap-2 sm:flex-row">
          <a href="/" class="btn btn-primary">Get funded now</a>
          <a href="/" class="btn btn-outline border-white/30 hover:bg-white/80">View products</a>
        </div>
      </div>
    </div>
  </section>

  <!-- What sets us apart -->
